{{> header }}
<h2>Customer Uploads</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Docs</th>
      <th scope="col">Upload Reason</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Submitted</th>
    </tr>
  </thead>
  <tbody data-testid="staff-dropboxes-list">
    {{#each dropboxes}}
      <tr>
        <td scope="row"><a href="/dropboxes/{{this.dropboxId}}/view">{{this.customerName}}</a></td>
        <td>{{this.count}}</td>
        <td>{{this.description}}</td>
        <td>{{this.customerEmail}}</td>
        <td>{{this.customerPhone}}</td>
        <td>{{dateFormat this.submitted}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{> footer isStaff=true }}