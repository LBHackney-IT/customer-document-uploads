{
  "name": "customer-document-uploads",
  "version": "1.0.0",
  "description": "A simple tool to enable Hackney residents to upload documents for Hackney staff to view",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "sls offline start -s dev --noAuth",
    "deploy": "sls deploy --aws-profile docuploads --stage production",
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline -s dev --noAuth",
    "dynamo": "sls dynamodb start",
    "lint": "eslint --fix --ext js ./*.js && echo 'Lint complete.'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBHackney-IT/customer-document-uploads.git"
  },
  "author": "Ben Pirt and Matthew Millar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LBHackney-IT/customer-document-uploads/issues"
  },
  "homepage": "https://github.com/LBHackney-IT/customer-document-uploads#readme",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "serverless": "^1.67.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^5.12.1",
    "serverless-offline-ssm": "^4.1.2",
    "serverless-s3-local": "^0.5.4"
  },
  "dependencies": {
    "@sentry/node": "^5.15.2",
    "aws-lambda-multipart-parser": "^0.1.3",
    "cookie": "^0.4.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "serverless-http": "^2.3.2"
  }
